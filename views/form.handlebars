<div class = "container">
    <nav>
        <a href="{{back}}">
            Back
        </a>
    </nav>

    <div class="resize">    
        <img src="/images/fakeEntry.png" alt="" id="logo" class="image">
    </div>
    <div class="display">
        
        <form action="{{api}}" method="POST" class="my-form">
            <strong>{{name}}</strong>
            <input type="hidden" id="name" name="name" value={{name}}>
            <div class="form-group">
                <p><label for="ic">NRIC/FIN</label></p>
                <input type="text" name="ic" class="ic">
            </div>
            <div class="form-group">
                <p><label for="number">Mobile Number</label></p>
                <input type="number" name="number" class="number">
            </div>
            <input class="button" type="submit" value="{{checktype}}" name="">
            {{#if msg}}
                <span class="error">{{msg}}</span>
            {{/if}}
        </form>
    </div>
</div>
